er":127,"pokedex":{"name":"updated-akala","url":"https://pokeapi.co/api/v2/pokedex/23/"}},{"entry_number":134,"pokedex":{"name":"isle-of-armor","url":"https://pokeapi.co/api/v2/pokedex/28/"}}],"shape":{"name":"squiggle","url":"https://pokeapi.co/api/v2/pokemon-shape/2/"},"varieties":[{"is_default":true,"pokemon":{"name":"palossand","url":"https://pokeapi.co/api/v2/pokemon/770/"}}]}